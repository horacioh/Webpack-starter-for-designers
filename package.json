  "homepage": "https://github.com/ArmendGashi/Webpack-starter-for-designers",
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-core": "^6.1.2",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "css-loader": "^0.22.0",
    "eslint": "^1.9.0",
    "eslint-loader": "^1.1.1",
